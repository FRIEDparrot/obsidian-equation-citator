{
	"name": "equation-citator",
	"version": "1.3.2",
	"description": "An Equation Citator Allows you to cite equations in your Obsidian notes.",
	"main": "main.js",
	"scripts": {
		"build:css": "sass src/styles/styles.scss:styles.css",
		"watch:css": "sass --watch src/styles/styles.scss:styles.css",
		"dev:esbuild": "node esbuild.config.mjs",
		"dev": "powershell -Command \"Start-Process cmd '/c npm run watch:css' -WindowStyle Hidden; npm run dev:esbuild\"",
		"test": "npx jest",
		"dbg": "npx jest ./tests/dbg.test.ts",
		"build": "tsc -noEmit --skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"check": "eslint .",
		"type-check": "tsc --noEmit --skipLibCheck",
		"check:case": "case-police"
	},
	"keywords": [],
	"author": "FRIEDparrot",
	"license": "Apache-2.0",
	"devDependencies": {
		"@codemirror/autocomplete": "^6.18.6",
		"@codemirror/language": "^6.11.2",
		"@codemirror/state": "^6.5.0",
		"@codemirror/view": "^6.38.1",
		"@eslint/config-array": "^0.21.0",
		"@eslint/js": "^9.39.1",
		"@lezer/markdown": "^1.4.3",
		"@types/jest": "^30.0.0",
		"@types/markdown-it": "^14.1.2",
		"@types/node": "^16.18.126",
		"@typescript-eslint/eslint-plugin": "^8.46.4",
		"@typescript-eslint/parser": "^8.46.4",
		"@typescript-eslint/utils": "^8.46.4",
		"boolean": "^3.2.0",
		"case-police": "^2.0.0",
		"esbuild": "^0.25.8",
		"eslint": "^9.39.1",
		"eslint-plugin-obsidianmd": "^0.1.8",
		"jest": "^30.2.0",
		"jest-environment-jsdom": "^30.0.5",
		"jsdom": "^26.1.0",
		"obsidian": "latest",
		"obsidian-typings": "^3.13.2",
		"ts-jest": "^29.4.0",
		"ts-node": "^10.9.2",
		"tslib": "2.4.0",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"chevrotain": "^11.1.1",
		"sass": "^1.89.2"
	}
}
